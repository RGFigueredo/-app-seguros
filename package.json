{
  "name": "app-seguros",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "build": "ng build --prod --output-hashing none",
    "build-local": "ng build --prod --aot --base-href /app-seguros/",
    "build-extra": "ng build --prod  --extraWebpackConfig webpack.extra.js --single-bundle true",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "npx-build-plus:copy-assets": "node copy-wc-polyfill.js && node copy-bundles.js",
    "build:externals": "ng build --extra-webpack-config webpack.externals.js --prod --single-bundle --output-hashing none",
    "build:app-seguros:externals": "ng build --extra-webpack-config webpack.externals.js --prod --project app-seguros --single-bundle --output-hashing none"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.3",
    "@angular/cdk": "~8.2.0",
    "@angular/common": "~8.0.3",
    "@angular/compiler": "~8.0.3",
    "@angular/core": "~8.0.3",
    "@angular/elements": "^8.0.3",
    "@angular/forms": "~8.0.3",
    "@angular/material": "~8.2.0",
    "@angular/platform-browser": "~8.0.3",
    "@angular/platform-browser-dynamic": "~8.0.3",
    "@angular/router": "~8.0.3",
    "@webcomponents/custom-elements": "^1.4.1",
    "@webcomponents/webcomponentsjs": "^2.4.3",
    "copy": "^0.3.2",
    "cors": "^2.8.5",
    "document-register-element": "^1.7.2",
    "ngx-build-plus": "^8.1.4",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.6",
    "@angular/cli": "~8.0.6",
    "@angular/compiler-cli": "~8.0.3",
    "@angular/language-service": "~8.0.3",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
}
